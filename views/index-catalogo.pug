extends catalogo

block content
	.container
		.row
			.col-md-4
				.mapa
					a(href='http://localhost:5000/') Home 
				p.caminho / Catalogo
				.filtros
					.row
						p O que você deseja ver? clique para encontrar
					.row
						.col-md-4
							img(src="../images/iconbolo.png")
						.col-md-5
							p#bolo Bolos 
					.row
						.col-md-4
							img(src="../images/iconboloformato.png")
						.col-md-6
							p#bolo Bolos com Formatos 
					.row
						.col-md-4
							img(src="../images/iconcupcake.png")
						.col-md-5
							p#bolo Cupcakes 					
					.row
						.col-md-4
							img(src="../images/icondoces.png")
						.col-md-5
							p#bolo Docinhos						
					.row
						.col-md-4
							img(src="../images/icontorta.png")
						.col-md-5
							p#bolo Torta 
			.col-md-7
				.row.titulo
					.col-md-12
						H1  Catalogo de Produtos
				.row
					- for (var x = 0; x < 9; x++)
						.col-md-4
							.moldura
								img(src= "../images/"+data[x].imag+".jpg")
								p= data[x].nome
								h5= "R$ "+data[x].preço